{
  "name": "daisy-vue",
  "version": "0.0.1",
  "files": [
    "dist"
  ],
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "build-lib": "vue-tsc -b ./tsconfig.lib.json && vite build",
    "type-check": "vue-tsc --build",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "format": "prettier --write --experimental-cli src/"
  },
  "dependencies": {
    "uuid": "^13.0.0",
    "vue": "^3.5.26"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@tsconfig/node24": "^24.0.3",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.4",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "daisyui": "^5.5.14",
    "jsdom": "^27.3.0",
    "npm-run-all2": "^8.0.4",
    "oxlint": "^1.36.0",
    "prettier": "3.7.4",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@latest",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.2.1"
  },
  "sideEffects": [
    "*.css"
  ]
}
